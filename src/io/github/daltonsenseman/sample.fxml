<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  prefHeight="480.0" prefWidth="720.0" tabClosingPolicy="UNAVAILABLE"
  xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="io.github.daltonsenseman.Controller">
  <tabs>
    <Tab text="Product Line">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <GridPane layoutX="30.0" layoutY="32.0" prefHeight="119.0" prefWidth="546.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.0" minWidth="10.0"
                  prefWidth="156.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="390.0" minWidth="10.0"
                  prefWidth="390.0"/>
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
              </rowConstraints>
              <children>
                <Label text="Product Name:">
                  <font>
                    <Font size="20.0"/>
                  </font>
                </Label>
                <Label text="Manufacturer:" GridPane.rowIndex="1">
                  <font>
                    <Font size="20.0"/>
                  </font>
                </Label>
                <Label text="Item Type:" GridPane.rowIndex="2">
                  <font>
                    <Font size="20.0"/>
                  </font>
                </Label>
                <TextField fx:id="productNameTBox" GridPane.columnIndex="1"/>
                <TextField fx:id="manufacturerTBox" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <ChoiceBox fx:id="itemTypeCBox" prefHeight="25.0" prefWidth="96.0"
                  GridPane.columnIndex="1" GridPane.rowIndex="2"/>
              </children>
            </GridPane>
            <Button fx:id="addProduct" layoutX="30.0" layoutY="168.0" mnemonicParsing="false"
              onAction="#addProduct" prefHeight="36.0" prefWidth="124.0" text="Add Product"/>
            <TableView layoutX="30.0" layoutY="210.0" prefHeight="226.0" prefWidth="545.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1"/>
                <TableColumn prefWidth="75.0" text="C2"/>
              </columns>
            </TableView>
            <Label layoutX="218.0" layoutY="173.0" prefHeight="37.0" prefWidth="257.0"
              text="Existing Products">
              <font>
                <Font size="20.0"/>
              </font>
            </Label>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab text="Produce">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <ListView layoutX="26.0" layoutY="57.0" prefHeight="291.0" prefWidth="303.0"/>
            <Label layoutX="26.0" layoutY="27.0" text="Choose Product">
              <font>
                <Font size="20.0"/>
              </font>
            </Label>
            <Label layoutX="26.0" layoutY="353.0" text="Choose Quantity">
              <font>
                <Font size="20.0"/>
              </font>
            </Label>
            <ComboBox fx:id="productQCombo" layoutX="25.0" layoutY="392.0" prefWidth="150.0"/>
            <Button fx:id="recordButton" layoutX="214.0" layoutY="392.0" mnemonicParsing="false"
              onAction="#recordProduction" text="Record Production"/>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab text="Production Log">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <TextArea layoutX="14.0" layoutY="14.0" prefHeight="419.0" prefWidth="694.0"/>
          </children>
        </AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
